#include <iostream>
#include <string>
#include <stdlib.h>
#include <iomanip> 

#include "Time.h"

Time::Time()
{
hours = 0;
minutes= 0;
seconds = 0;

}



Time::Time(int h, int m, int s)
{
hours = h;
minutes = m;
seconds = s;
}

Time::~Time(){}

int Time::getHour()
{
return hours;
}

int Time::getMinute()
{
return minutes;
}

int Time::getSecond()
{
return seconds;
}

void Time::setHours(int h)
{
hours = h;
}

void Time::setMinutes(int m)
{
minutes = m;
}

void Time::setSeconds(int s)
{
seconds = s;
}





